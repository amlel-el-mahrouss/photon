{
  "compiler_path": "g++",
  "compiler_std": "c++20",
  "headers_path": [
    "./dev/webdll/",
    "./vendor/",
    "/opt/homebrew/opt/openssl@3.3/include"
  ],
  "sources_path": ["dev/dll/*.cpp"],
  "output_name": "libwebenine.dylib",
  "compiler_flags": [
    "-fPIC",
    "-shared",
    "-L/opt/homebrew/opt/openssl@3.3/lib",
    "-lssl",
    "-lcrypto"
  ],
  "cpp_macros": [
    "__ZKA_WEB__",
    "cWebVersion=0x0100",
    "cWebVersionHighest=0x0100",
    "cWebVersionLowest=0x0100"
  ]
}
