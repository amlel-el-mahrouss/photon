{
  "compiler_path": "g++",
  "compiler_std": "c++20",
  "headers_path": ["./", "../../vendor"],
  "sources_path": ["src/*.cc"],
  "output_name": "libVitoSDK.so",
  "compiler_flags": ["-fPIC", "-shared"],
  "cpp_macros": [
    "__VITO__",
    "__AMD64_VITO__",
    "cVitoVersion=0x0100",
    "cVitoVersionHighest=0x0100",
    "cVitoVersionLowest=0x0100"
  ]
}
