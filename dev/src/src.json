{
  "compiler_path": "g++",
  "compiler_std": "c++20",
  "headers_path": ["./", "../../vendor"],
  "sources_path": ["src/*.cc"],
  "output_name": "libZircon.so",
  "compiler_flags": ["-fPIC", "-shared"],
  "cpp_macros": [
    "__ZIRCON__",
    "__AMD64_ZIRCON__",
    "cZWMVersion=0x0100",
    "cZWMVersionHighest=0x0100",
    "cZWMVersionLowest=0x0100"
  ]
}
