{
  "compiler_path": "g++",
  "compiler_std": "c++20",
  "headers_path": [
    "../dll/",
    "../../vendor",
    "./vendor/",
    "/opt/homebrew/opt/openssl@3.3/include"
  ],
  "sources_path": ["*.cpp"],
  "output_name": "zexplore.exe",
  "compiler_flags": [
    "-L/opt/homebrew/opt/openssl@3.3/lib",
    "-lssl",
    "-lcrypto",
    "-L/usr/local/lib/",
    "-framework AppKit",
    "-lSero"
  ],
  "cpp_macros": [
    "__WEB_ENGINE__",
    "cWebVersion=0x0100",
    "cWebVersionHighest=0x0100",
    "cWebVersionLowest=0x0100"
  ]
}
